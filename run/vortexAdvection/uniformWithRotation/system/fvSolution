FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "Phi|mmPhi"
    {
        solver GAMG;
        smoother symGaussSeidel;
        preconditioner   DILU;
        tolerance        1e-4;
        relTol           0.01;
        maxIter          10;
    };
    streamFunction
    {
        solver PCG;
        preconditioner DIC;
        tolerance 1e-7;
        relTol 0;
        maxIter 500;
    }
}

