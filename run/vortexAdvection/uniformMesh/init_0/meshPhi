FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "0";
    object      meshPhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    ground
    {
        type            fixedValue;
        value           uniform 0;
    }
    top
    {
        type            fixedValue;
        value           uniform 0;
    }
    inlet
    {
        type            cyclic;
        value           uniform 0;
    }
    outlet
    {
        type            cyclic;
        value           uniform 0;
    }
    front
        {
            type empty;
        }
    back
        {
            type empty;
        }
}


