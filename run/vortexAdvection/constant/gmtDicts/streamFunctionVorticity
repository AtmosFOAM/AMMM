FieldsToPlot
(
    [vorticity filledContours -0.0475 0.0475 0.005 "$GMTU/colours/red_white_blue.cpt"]
    [none mesh "grey"]
    [streamFunction contours -2.5e5 2.5e5 2.5e4 "black"]
    [Uf vectors 20 5 "black"]
);

