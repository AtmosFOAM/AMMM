FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Euler;
}
offCentre 0.5;

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default none;
    div(U,u)   Gauss linear; //linearUpwind linearUpwind;
}

laplacianSchemes
{
    default         none;
    laplacian(streamFunction) Gauss linear corrected;
    laplacian(deltaT,p)       Gauss linear uncorrected;
    laplacian((1|sqr(deltaCoeffs)),monitor) Gauss linear uncorrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         none;
    snGrad(p)       uncorrected;
}

fluxRequired
{
    default         no;
    p;
}


// ************************************************************************* //
