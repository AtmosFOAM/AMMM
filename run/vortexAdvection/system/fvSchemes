FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Euler;
}
offCentre 0.6;

gradSchemes
{
    default         none;
    linearUpwind     Gauss linear;
    grad(phi)       Gauss linear;
    grad(mmphi)       Gauss linear;
    grad(mmPhi)       leastSquares;
    grad(gradmmPhif)  Gauss linear;
    grad(p)         Gauss linear;
}

divSchemes
{
    default none;
    div(U,u)   Gauss linearUpwind linearUpwind;
    div(phi,U)      Gauss linear;// only for icoFoam
    div((S&gradc_m,phi) Gauss linear;
}

laplacianSchemes
{
    default         Gauss midPoint uncorrected;
}

interpolationSchemes
{
    default         linear;
    interpolate(convection(U,u)) linearUpwind U linearUpwind;
    interpolate(u)  midPoint;
    H               midPoint;
    interpolate(gradmmPhi)    linear;
    flux(U) linear; //only for icoFoam
    dotInterpolate(S,U_0) linear;//only for icoFoam
    interpolate((1|A(U))) linear;//only for icoFoam
    flux(HbyA) linear;//only for icoFoam
}

snGradSchemes
{
    default         none;
    snGrad(p)   uncorrected;
    snGrad(mmPhi) uncorrected;
}

fluxRequired
{
    default         no;
    p;
}


// ************************************************************************* //
