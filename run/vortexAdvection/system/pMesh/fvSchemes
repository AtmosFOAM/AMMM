FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Euler;
}
offCentre 0.6;

gradSchemes
{
    default         none;
    linearUpwind     Gauss linear;
    curl(u)          Gauss linear;
    grad(p)         Gauss linear;
}

divSchemes
{
    default none;
    div(U,u)   Gauss linearUpwind linearUpwind;
    div(phi,u) Gauss linearUpwind linearUpwind;
    div((U_0-meshPhi_0),u_0) Gauss linearUpwind linearUpwind;
    div(u)     Gauss linear;
}

laplacianSchemes
{
    default         none;
    laplacian(monitorP) Gauss linear uncorrected;
    laplacian(p)        Gauss linear corrected;
}

interpolationSchemes
{
    default         none;
    interpolate(convection(U,u)) linearUpwind U linearUpwind;
    interpolate(convection(phi,u)) linearUpwind phi linearUpwind;
    interpolate(convection((U_0-meshPhi_0),u_0)) linearUpwind U linearUpwind;
    interpolate(u)  midPoint;
    H               midPoint;
}

snGradSchemes
{
    default         none;
    snGrad(p)   corrected;
}

fluxRequired
{
    default         no;
    p;
}


// ************************************************************************* //
