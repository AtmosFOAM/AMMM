FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver PCG;
        preconditioner DIC;
        tolerance        1e-8;
        relTol           0.00001;
        maxIter         80;
    }

    pFinal
    {
        $p;
        tolerance        1e-8;
        relTol           0;
    }
    u
        {
            solver PCG;
            preconditoner DIC;
            tolerance 1e-7;
            relTol 0.0001;
            maxIter 500;
        }
}

iterations
{
    nCorrectors      1;
    nNonOrthogonalCorrectors 40;
    uPredictor       false;
}

setReferenceP true;

// ************************************************************************* //
