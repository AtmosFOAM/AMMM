FoamFile
{
    version         2.3;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XZ;

FieldsToPlot
(
    [streamFunction contours -2.5e5 2.5e5 5e4 "grey"]
    [Uf vectors 100 11 "black"]
);

//separateLegend true;
region "-25e3/25e3/-25e3/25e3";
annotate "minMax -7.5e3 -5000";
boundaryMarks "5e3/5e3";

