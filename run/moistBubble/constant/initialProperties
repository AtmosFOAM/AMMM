// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// inputs for makeHotBubble
thetaPrime 0; //0.006666667;
bubbleCentre (0 500 2e3);
bubbleRadii  (2000 1 2000);

thetae thetae [0 0 0 1 0] 320;
totalWaterRatio totalWaterRatio [0 0 0 0 0] 0.02;
maxIters 25;
BCiters 40;
BCtol 1e-4;
boundaryRelaxation 0.5;
// under-relaxation of rv (1 means no under-relaxation)
underRelax 0.5;

