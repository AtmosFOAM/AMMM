FoamFile
{
    version         1.4;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XZ;

FieldsToPlot
(
    [Rm filledContours 0 0 0 "$GMTU/colours/red_white_blue.cpt"]
);

annotate "minMax -1e3 -100";
