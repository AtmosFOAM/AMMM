FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          tracerDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Specify the initial tracer
type      cosine;
width     500;
centre    (5e3 7.5e3 500);
maxTracer 1;

