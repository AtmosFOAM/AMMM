FoamFile
{
    version         1.4;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XY;
plotBoundaryDots false;

FieldsToPlot
(
    [monitor filledContours 1 4 0.1 "$GMTU/colours/white_blue_red.cpt"]
    [none mesh "0.25,black"]
    [V contours 10e6 38e6 4e6 "2,black,10_5:0"]
    [V contours 42e6 100e6 4e6 "2,black"]
);

