FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Euler; //CrankNicolson 1;
}
offCentre 0.5;

gradSchemes
{
    default         none;
    linearUpwind    Gauss linear;
    grad(T)         Gauss linear;
}

divSchemes
{
    default         none;
    div(phi,T)      Gauss linearUpwind linearUpwind;
}

laplacianSchemes
{
    default         none;
}

interpolationSchemes
{
    default                 none;
    interpolate(grad(T))    linear;
}

snGradSchemes
{
    default         none;
}

fluxRequired
{
    default         no;
    h;
}


