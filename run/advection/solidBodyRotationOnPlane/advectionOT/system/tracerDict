FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          tracerDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Specify the initial tracer
type      gaussian;
width     500;
centre    (10e3 12.5e3 500)
maxTracer 1;

