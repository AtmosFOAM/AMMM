FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      A;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type            zeroGradient;
        value           uniform 1;
    }
    outlet
    {
        type            zeroGradient;
        value           uniform 1;
    }
    equator
    {
        type            zeroGradient;
        value           uniform 1;
    }
    pole
    {
        type            zeroGradient;
        value           uniform 1;
    }
    top
    {
        type            zeroGradient;
        value           uniform 1;
    }
    ground
    {
        type            zeroGradient;
        value           uniform 1;
    }
}


