FoamFile
{
    version         1.4;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XY;
plotBoundaryDots false;

FieldsToPlot
(
    [monitor filledContours 1 2 0.1 "$GMTU/colours/white_blue_red.cpt"]
    [none mesh "grey"]
);

// Options
//region "0.01/0.9999e4/0/0.9999e4";
//region "0.01/1.9999e4/0/2.01e4";
annotate "minMax 0 -38000";
