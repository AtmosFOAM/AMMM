FoamFile
{
    version         1.4;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XY;
plotBoundaryDots false;

FieldsToPlot
(
    [monitor filledContours 1 4 0.25 "$GMTU/colours/white_blue_red.cpt"]
    [none mesh "grey"]
    [Uf vectors 1e4 11 "black"]
);

// Options
region "0.01/0.9999e4/0/1.01e4";
annotate "minMax 0 -38000";
