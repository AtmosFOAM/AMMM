FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/pMesh";
    object      uniT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    south
    {
        type            cyclic;
        value           uniform 0.0;
    }
    north
    {
        type            cyclic;
        value           uniform 0.0;
    }
    east
    {
        type            cyclic;
        value           uniform 0.0;
    }
    west
    {
        type            cyclic;
        value           uniform 0.0;
    }
    top
    {
        type            zeroGradient;
    }
    ground
    {
        type            zeroGradient;
    }
}


