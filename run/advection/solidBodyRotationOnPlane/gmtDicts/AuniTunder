FoamFile
{
    version         1.4;
    format          ascii;

    root            "/export/lime/data-04/sws02hs/OpenFOAM/sws02hs-1.4.1/run/shallowWater";
    case            "bucky4_30";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
projection "X18c/18c";
projection2D XY;

FieldsToPlot
(
    [AuniT filledContours 0.455 0.545 0.01 "$GMTU/colours/red_white_blue.cpt"]
    [T contours 0.1 1 0.1 "black"]
);

// options
region "0.01/0.9999e4/0/1.01e4";
//boundaryMarks "a1e3/a1e3";
//separateLegend true;
//overlay true;
plotBoundaryDots false;
annotate "minMax 2000 500";
lastPlot false;
overlay false;
