FoamFile
{
    version         1.4;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XY;

FieldsToPlot
1
(
    [none mesh "black"]
);

// Options
// For 3d plots, must give the patch to plot
patch back;

