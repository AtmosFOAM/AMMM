FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    Phi
    {
        solver smoothSolver;
        smoother GaussSeidel;
        preconditioner   DILU;
        tolerance        1e-5;
        relTol           0.05;
        maxIter          2000;
    };
}

relaxationFactors
{
    equations
    {
        Phi               0.999;
    }
}
