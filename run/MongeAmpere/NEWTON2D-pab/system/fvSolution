FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "Phi|phi"
    {
        solver BICCG;
        smoother symGaussSeidel;
        preconditioner   DILU;
        tolerance        1e-12;
        relTol           1e-8;
        maxIter          500;
    };
}

relaxationFactors
{
    equations
    {
        Phi               0.999;
        phi               0.999;
    }
}
// ************************************************************************* //

