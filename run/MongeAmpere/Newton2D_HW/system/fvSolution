FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    phi
    {
        solver BICCG;
        preconditioner DILU;
        tolerance        0;
        relTol           1e-6;
        maxIter          100;
    };
}

relaxationFactors
{
    equations
    {
        phi               0.99;
    }
}
