FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Euler;
}
offCentre 0.5;

gradSchemes
{
    default         none;
    grad(Phi)   Gauss linear;
}

divSchemes
{
    default             none;
}

laplacianSchemes
{
    default     none;
    laplacian(Phi) Gauss linear uncorrected;
    laplacian(sqr((1|deltaCoeffs)),monitorNew) Gauss linear uncorrected;
}

interpolationSchemes
{
    default        none;
    interpolate(gradPhi) linear;
}

snGradSchemes
{
    default         none;
    snGrad(Phi)     uncorrected;
}

fluxRequired
{
    default         no;
}


// ************************************************************************* //
