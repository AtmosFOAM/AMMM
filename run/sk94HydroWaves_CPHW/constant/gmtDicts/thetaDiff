FoamFile
{
    version         1.4;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/8c";
projection2D XZ;
plotBoundaryDots false;

FieldsToPlot
(
    [thetaDiff rawValues -0.005 0.005 0.0005 "$GMTU/colours/red_white_blue.cpt"]
    [thetaDiff contours -0.005 -0.5e-3 0.5e-3 "black,3_3:0"]
    [thetaDiff contours 0.5e-3 0.005 0.5e-3 "black"]
);

boundaryMarks "1e6/2e3WSen";
