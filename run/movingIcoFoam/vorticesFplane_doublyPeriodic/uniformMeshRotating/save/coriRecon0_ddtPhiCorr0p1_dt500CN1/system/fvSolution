FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

setReferenceP true;

solvers
{
    "h|p"
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-8;
        relTol          0.01;
    }

    "hFinal|pFinal"
    {
        $p;
        relTol          0;
    }

    "U|UFinal|hU|hUFinal"
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-05;
        relTol          0;
    }
    "streamFunction|velPot"
    {
        solver         PCG;
        preconditioner DIC;
        tolerance      1e-7;
        relTol         0;
    }
}

"PISO|PIMPLE"
{
    nCorrectors     3;
    nNonOrthogonalCorrectors 0;
    pRefCell        0;
    pRefValue       0;
    momentumPredictor false;
}

