FoamFile
{
    version         2.3;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/9c";
projection2D XZ;

FieldsToPlot
(
    [vorticity filledContours -0.0475 0.0475 0.005 "$GMTU/colours/red_white_blue.cpt"]
    [vorticity2D filledContours -0.0475 0.0475 0.005 "$GMTU/colours/red_white_blue.cpt"]
    [Uf vectors 20 1 "black"]
    [none mesh "grey"]
);

separateLegend true;
region "-25e3/25e3/0/25e3";
annotate "minMax -7.5e3 -2500";
boundaryMarks "5e3/5e3";

