FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Euler;
}
offCentre 0.5;

gradSchemes
{
    default         none;
    LUST            leastSquares;
}

divSchemes
{
    default             none;
    div(U,u)            Gauss LUST LUST;
}

laplacianSchemes
{
    default         none;
    h Gauss linear uncorrected;
}

interpolationSchemes
{
    default        none;
    interpolate(h) linear;
    Uf linear;
    H  linear;
}

snGradSchemes
{
    default         none;
    snGrad(h)       uncorrected;
}

fluxRequired
{
    default         no;
    h;
}


