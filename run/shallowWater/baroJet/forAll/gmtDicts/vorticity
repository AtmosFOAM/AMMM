FoamFile
{
    version         1.4;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/6c";
projection2D XY;
plotBoundaryDots false;

FieldsToPlot
(
    [vorticity filledContours -1.05e-4 1.05e-4 1e-5 "$GMTU/colours/red_white_blue.cpt"]
    //[vorticity filledContours 0 0 0 "$GMTU/colours/red_white_blue.cpt"]
    [vorticity2D filledContours -1.05e-4 1.05e-4 1e-5 "$GMTU/colours/red_white_blue.cpt"]
    [Uf vectors 2e4 19 "black"]
);

separateLegend true;
