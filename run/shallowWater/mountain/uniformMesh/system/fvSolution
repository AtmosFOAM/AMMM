FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    h
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-8;
        relTol          0.01;
    }
    hFinal
    {
        $h;
        relTol          0;
    }
    "h0|muSponge"
    {
        solver       none;
    }
}

"PISO|PIMPLE"
{
    nCorrectors     3;
    nNonOrthogonalCorrectors 0;
    momentumPredictor false;
}

initialisation
{
    maxIters 20;
}

