FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default CrankNicolson 0.8;
}
ACblend       0;
CoriRecon     0;

gradSchemes
{
    default         none;
    linearUpwind    leastSquares;
    grad(h)         Gauss linear;
    grad(h0)        Gauss linear;
    grad(U)         Gauss linear;
}

divSchemes
{
    default         none;
    div(phi,U)      Gauss linearUpwind linearUpwind;
    div(meshPhi,h0) Gauss downwind;
    div(meshPhi,muSponge) Gauss downwind;
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default            linear;
}

snGradSchemes
{
    default         none;
    snGrad(h)       corrected;
    snGrad(h0)       corrected;
}

fluxRequired
{
    default         no;
    h;
}


