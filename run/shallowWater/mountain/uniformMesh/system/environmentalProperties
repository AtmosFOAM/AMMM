FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          environmentalProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

g g       [0 1 -2 0 0 0 0] 10;
OmegaHat (0 0 1);
beta beta [0 -1 -1 0 0] 1e-11;
f0   f0   [0  0 -1 0 0] 0;

// Coefficients for top and bottom sponge layers
nSponges 2;
spongeOnCellCentres true;
spongeCentre0 (0 0 0);
spongeLength0 (0 2e6 0);
spongeMax0    0.5;
spongeCentre1 (0 12e6 0);
spongeLength1 (0 2e6 0);
spongeMax1    0.5;

