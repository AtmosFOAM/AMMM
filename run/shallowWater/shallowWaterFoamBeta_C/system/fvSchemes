FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Euler;
}
offCentre 0.5;

gradSchemes
{
    default         none;
    grad(h)         leastSquares;
    grad(U)         leastSquares;
    curl(U)         leastSquares;
}

divSchemes
{
    default             none;
    div(flux,U)          Gauss LUST grad(U);
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default        none;
    interpolate(h) linear;
    interpolate(convection(flux,U)) linear;
    interpolate(U) linear;
}

snGradSchemes
{
    default         uncorrected;
}

fluxRequired
{
    default         no;
    h;
}


// ************************************************************************* //
