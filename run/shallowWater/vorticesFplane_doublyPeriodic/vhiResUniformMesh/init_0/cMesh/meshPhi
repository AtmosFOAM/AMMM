FoamFile
{
    version     2.0;
    format      binary;
    class       surfaceScalarField;
    location    "0";
    object      meshPhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    equator
    {
        type            cyclic;
        value           uniform 0;
    }
    pole
    {
        type            cyclic;
        value           uniform 0;
    }
    inlet
    {
        type            cyclic;
        value           uniform 0;
    }
    outlet
    {
        type            cyclic;
        value           uniform 0;
    }
    front
    {
        type            empty;
        value           nonuniform 
0
;
    }
    back
    {
        type            empty;
        value           nonuniform 
0
;
    }
}


