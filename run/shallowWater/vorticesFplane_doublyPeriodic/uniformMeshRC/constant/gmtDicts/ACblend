FoamFile
{
    version         1.4;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XY;

FieldsToPlot
(
    [h filledContours 8000 10400 60 "$GMTU/colours/red_white_blue.cpt"]
    [U vectors 1e4 1 "grey"]
    //[ACblend filledContours 0 1 0.05 "$GMTU/colours/white_blue_red.cpt"]
    [ACblend contours 0.01 1 0.1 "black"]
);

// Options
plotBoundaryDots false;
//separateLegend true;
//region "-25e3/25e3/0/25e3";
annotate "minMax 0 -38000";
//boundaryMarks "5e3/5e3";

