FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Euler; //CrankNicolson 0.8;
}

gradSchemes
{
    default         Gauss linear;
    linearUpwind Gauss linear;
    grad(streamFunction) Gauss linear;
    curl((streamFunction*(0,0,1))) Gauss linear;
    grad(h)         Gauss linear;
    grad((h+h0))    Gauss linear;
    curl(U)         Gauss linear;
}

divSchemes
{
    default none;
    div(phi,U)   Gauss linearUpwind linearUpwind;
    div(phiv,hU)   Gauss linearUpwind linearUpwind;
}

laplacianSchemes
{
    default         Gauss linear uncorrected;
    laplacian(streamFunction) Gauss linear uncorrected;
    laplacian(monitor)        Gauss linear uncorrected;
    laplacian(h)    Gauss linear uncorrected;
    laplacian((mag(g)*interpolate((h*(1|A(hU))))),h) Gauss linear corrected;
}

interpolationSchemes
{
    default            linear;
    interpolate((h*U)) linear;  
    interpolate(h)     linear;
    interpolate((h*(1|A(U)))) linear;
    dotInterpolate(S,(h_0*U_0)) linear;
    flux((h*HbyA))     linear;
}

snGradSchemes
{
    default         none;
    snGrad(h)       uncorrected;
    snGrad(h0)       uncorrected;
}

fluxRequired
{
    default         no;
    h;
}


// ************************************************************************* //
