FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Euler;
}
offCentre 0.6;

gradSchemes
{
    default         none;
    linearUpwind Gauss linear;
    grad(streamFunction) Gauss linear;
    curl((streamFunction*(0,0,1))) Gauss linear;
    grad(h) Gauss linear;
}

divSchemes
{
    default     none;
    div(U,u)    Gauss linearUpwind linearUpwind;
}

laplacianSchemes
{
    default         none;
    laplacian(streamFunction) Gauss linear corrected;
    h                         Gauss linear uncorrected;
    laplacian(monitor)        Gauss linear uncorrected;
}

interpolationSchemes
{
    default         none;
    interpolate(h)  linear;
    H               linear;
    Uf              linear;
    divuu           linear;
    interpolate(reconstructd((Ufone&delta))) linear;
    interpolate(volIntegrate((Ufone&S))) linear;
    interpolate(grad(h)) linear;
}

snGradSchemes
{
    default         none;
    snGrad(h)       uncorrected;
}

fluxRequired
{
    default         no;
    h;
}


// ************************************************************************* //
