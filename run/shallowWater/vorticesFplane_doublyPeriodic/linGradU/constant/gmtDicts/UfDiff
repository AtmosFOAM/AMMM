FoamFile
{
    version         1.4;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XY;
plotBoundaryDots false;

FieldsToPlot
(
    [fluxReconError rawValues -1e-4 1e-4 1e-5 "$GMTU/colours/red_white_blue.cpt"]
    [Ufrecon vectors 0 1 "black"]
    [UfDiff vectors 20 1 "red"]
);


